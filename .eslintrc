{
  "extends": "eslint:recommended",
  "env": {
    "node": true
  },
  "rules": {
    "curly": [2, "multi-line"],
    "dot-notation": [2, {
      "allowPattern": "^[a-z]+(_[a-z]+)+$"
    }],
    "eqeqeq": 2,
    "no-caller": 2,
    "no-labels": 2,
    "no-empty-pattern": 2,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-implied-eval": 2,
    "no-iterator": 1,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 1,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-proto": 1,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-self-compare": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-void": 2,
    "no-with": 2,
    "wrap-iife": [2, "inside"],
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-path-concat": 2,
    "array-bracket-spacing": 2,
    "block-spacing": 2,
    "brace-style": [2, "1tbs", {
      "allowSingleLine": true
    }],
    "comma-spacing": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "eol-last": 2,
    "indent": [2, 2, {
      "SwitchCase": 1
    }],
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "operator-assignment": 2,
    "quotes": [2, "single", "avoid-escape"],
    "semi-spacing": 2,
    "semi": 2,
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2, "never"],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "no-console": 0
  }
}

// vim: set ft=json:
